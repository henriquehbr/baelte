{
  "name": "baelte-cli",
  "version": "0.2.0",
  "main": "./lib/index.js",
  "author": "Henrique Borges <henriqueborgeshbr@gmail.com>",
  "license": "MIT",
  "preferGlobal": true,
  "scripts": {
    "clean-build": "rimraf lib",
    "copy-templates": "copyfiles -u 1 ./src/templates/**/* ./lib",
    "replace-plopfile-ext": "replace-in-file /\\.ts/g .js lib/*.js --isRegex",
    "start": "node lib",
    "dev": "ts-node -r tsconfig-paths/register src",
    "prebuild": "yarn clean-build",
    "build": "ttsc",
    "postbuild": "yarn copy-templates && yarn replace-plopfile-ext"
  },
  "bin": {
    "baelte": "./lib/index.js"
  },
  "devDependencies": {
    "@types/cross-spawn": "^6.0.2",
    "@types/edit-json-file": "^1.4.0",
    "@types/eslint": "^7.2.1",
    "@types/inquirer": "^7.3.1",
    "@types/node": "^14.6.0",
    "copyfiles": "^2.3.0",
    "eslint": "^7.7.0",
    "replace-in-file": "^6.1.0",
    "rimraf": "^3.0.2",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "ttypescript": "^1.5.11",
    "typescript": "3.9.2",
    "typescript-transform-paths": "^2.0.0"
  },
  "dependencies": {
    "cross-spawn": "^7.0.3",
    "edit-json-file": "^1.4.1",
    "plop": "^2.7.4",
    "plop-pack-git-init": "^0.3.1"
  }
}
